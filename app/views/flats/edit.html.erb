<h1>Flats#edit</h1>
<p>Find me in app/views/flats/edit.html.erb</p>

<%= form_for @flat do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label(:furnished, "Furnished?") %>
  <%= f.check_box(:furnished) %>

  <%= f.submit "Submit" %>

<% end %>
